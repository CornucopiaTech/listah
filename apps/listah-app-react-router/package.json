{
  "name": "listah-app-react-router",
  "private": true,
  "type": "module",
  "scripts": {
    "app:build": "cd ./apps/listah-app-react-router && clear && cross-env NODE_ENV=production react-router build",
    "app:dev": "clear && react-router dev",
    "app:build-start": "clear && cross-env NODE_ENV=production react-router build && react-router-serve ./build/server/index.js",
    "app:start": "clear && cross-env NODE_ENV=production react-router-serve ./build/server/index.js",
    "app:typecheck": "react-router typegen && tsc"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.3.1",
    "@mui/material": "^6.3.1",
    "@react-router/node": "^7.1.1",
    "@react-router/serve": "^7.1.1",
    "@redocly/cli": "^1.27.1",
    "@reduxjs/toolkit": "^2.5.0",
    "@tabler/icons-react": "^3.6.0",
    "cross-env": "^7.0.3",
    "isbot": "^5.1.17",
    "next": "^15.0.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-redux": "^9.2.0",
    "react-router": "^7.1.1",
    "react-window": "^1.8.10",
    "rxjs": "^7.8.1",
    "tslib": "^2.7.0",
    "webpack": "^5.95.0"
  },
  "packageManager": "npm@10.9.0",
  "devDependencies": {
    "@faker-js/faker": "^9.1.0",
    "@react-router/dev": "^7.1.1",
    "@types/node": "^20",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^8",
    "eslint-config-next": "14.2.15",
    "nodemon": "^3.1.9",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.16",
    "typescript": "^5.7.2",
    "vite": "^5.4.11",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
